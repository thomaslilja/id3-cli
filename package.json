{
  "name": "id3-cli",
  "version": "0.2.4",
  "description": "Command Line Interface to get and set ID3 meta tags on MP3's.",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel ./src -d ./lib",
    "dev": "npm run build -- -w",
    "lint": "eslint ./src",
    "test": "npm-run-all test:*",
    "test:unit": "nyc mocha --require @babel/register",
    "test:audit": "audit-ci --high",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "id3-cli": "./lib/index.js",
    "id3": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ThomasLilja/id3-cli.git"
  },
  "keywords": [
    "npm",
    "node",
    "javascript",
    "id3"
  ],
  "author": "Thomas Lilja",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ThomasLilja/id3-cli/issues"
  },
  "homepage": "https://github.com/ThomasLilja/id3-cli#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "clui": "^0.3.6",
    "commander": "^2.20.3",
    "figlet": "^1.3.0",
    "inquirer": "^6.5.2",
    "lodash.foreach": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "node-id3": "^0.1.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "audit-ci": "^3.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.20.2",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
